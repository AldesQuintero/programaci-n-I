#include <stdio.h>
#include <conio.h>
int main (){
int L1T1, A1T1,L2T1,A2T1,L3T1,A3T1,L1T2, A1T2,L2T2,A2T2,L3T2,A3T2;
printf("Ingrese el valor de su lado 1 del primer triangulo ");
scanf("%d",&L1T1);
printf("Ingrese el valor de su lado 2 del primer triangulo ");
scanf("%d",&L2T1);
printf("Ingrese el valor de su lado 3 del primer triangulo ");
scanf("%d",&L3T1);
printf("Ingrese el valor de su angulo 1 del primer triangulo ");
scanf("%d",&A1T1);
printf("Ingrese el valor de su angulo 2 del primer triangulo ");
scanf("%d",&A2T1);
printf("Ingrese el valor de su angulo 3 del primer triangulo ");
scanf("%d",&A3T1);
printf("Ingrese el valor de su lado 1 del segundo triangulo ");
scanf("%d",&L1T2);
printf("Ingrese el valor de su lado 2 del segundo triangulo ");
scanf("%d",&L2T2);
printf("Ingrese el valor de su lado 3 del segundo triangulo ");
scanf("%d",&L3T2);
printf("Ingrese el valor de su angulo 1 del segundo triangulo ");
scanf("%d",&A1T2);
printf("Ingrese el valor de su angulo 2 del segundo triangulo ");
scanf("%d",&A2T2);
printf("Ingrese el valor de su angulo 3 del segundo triangulo ");
scanf("%d",&A3T2);
if (L1T1==L1T2 && L2T1==L2T2 && L3T1==L3T2 && A1T1==A1T2 &&
A2T1==A2T2 && A3T1==A3T2){
printf("Tus triangulos son congruentes");
}else{
printf("Tus triangulos no son congruentes");
}
