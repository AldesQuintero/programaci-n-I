#include <stdio.h>
#include <stdlib.h>
#include <conio.h>
int main()
{
 int a[7][4],i,j,sum;
 for (j=0;j<4;j++)
 {
 sum=0;
 for (i=0;i<5;i++)
 {
 a[i][j]=(i*4)+1+j;/*0*5=0+1=1+4=5*/
 sum += a[i][j];
 }
 system("color 4F");{
 a[5][j]=sum;}
 }

 for (i=0;i<6;i++)
 {
 for (j=0;j<4;j++)
 {
 printf("%d ",a[i][j]);

 }
 printf("\n");
 }

 system("PAUSE");
 return 0;
}
