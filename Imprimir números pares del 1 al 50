#include <stdio.h>
 void main()
 {
 int array[100], i, num;
 printf("Ingrese el tamano de un array \n");
 scanf("%d", &num);
 printf("Ingrese los elementos del array\n");
 for (i = 0; i < num; i++)
 {
 scanf("%d", &array[i]);
 }
 printf("Los numeros pares en el array : ");
 for (i = 0; i < num; i++)
 {
 if (array[i] % 2 == 0)
 {
 printf("%d \t", array[i]);
 }
 }
 
 }
