#include <stdio.h> 

#include <stdlib.h> 

 

int main (void) { 

     

    int i, numero, resultado;  

 

printf("Ingrese un numero: "); 

scanf("%d", &numero); 

putchar ('\n'); 

for (i=1; i<=10; i++) 

{ 

    resultado= 1 * numero; 

     printf( "\n   %d * %d = %d", i, numero, i * numero ); 

 

} 

putchar ('\n') ; 

return EXIT_SUCCESS; 

} 
