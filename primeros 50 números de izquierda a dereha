#include <stdio.h>
#include <stdlib.h>
int main(int argc, char *argv[])
{
 int x,y, numeros[10][10];

 for (x=0;x<6;x++)
 {
 for (y=0;y<30;y++)
 {
 numeros[x][y]=(x*4)+1+y;
 }

 }

 for (x=0;x<6;x++)
 {
 for (y=0;y<30;y++)
 {
 printf("%d ",numeros[x][y]);
 }
 printf("\n");
 }

 system("PAUSE");
 return 0;
}
